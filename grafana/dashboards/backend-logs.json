{
  "dashboard": {
    "title": "Backend Logs Dashboard",
    "tags": ["logs", "backend"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Log Volume by Level",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (level) (count_over_time({job=\"backend\"}[1m]))",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Recent Logs",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"backend\"}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 10,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate({job=\"backend\"} |= \"ERROR\" [5m]))",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      }
    ],
    "refresh": "10s",
    "schemaVersion": 27,
    "version": 1
  }
}

